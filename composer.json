{
    "name": "vicinity-media/ad2cart-magento",
    "description": "Enable customers to add items to their shopping cart on Magento, directly from a digital catalog.",
    "type": "magento2-module",
    "keywords": [
        "magento2"
    ],
    "license": "proprietary",
    "support": {
        "email": "me@shaughn.pro"
    },
    "authors": [
        {
            "name": "Shaughn Le Grange - Hatlen",
            "email": "me@shaughn.pro"
        }
    ],
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "VicinityMedia\\Ad2Cart\\": "src"
        }
    },
    "repositories": [
        { "type": "composer", "url": "https://repo.magento.com/" }
    ],
    "require": {
        "magento/framework": "^103.0.0"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "^2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "magento/magento-coding-standard": "*",
        "magento/magento2-functional-testing-framework": "^4.2.1",
        "pdepend/pdepend": "^2.10",
        "phpmd/phpmd": "^2.12",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "~3.6.0",
        "phpro/grumphp-shim": "^1.13",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "symfony/filesystem": "^5.4.11",
        "symfony/finder": "^5.4",
        "povils/phpmnd": "^3.0",
        "enlightn/security-checker": "^1.10"
    },
    "extra": {
        "grumphp": {
            "config-default-path": "./grumphp.yml"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "magento/*": true,
            "phpro/grumphp": true,
            "phpro/grumphp-shim": true
        }
    }
}
